{
  "name": "eause",
  "version": "0.0.4",
  "description": "Eause CSS Framework",
  "main": "index.js",
  "scripts": {
    "test": "echo \"No test specified\"",
    "watch:sass": "node-sass src/sass/main.scss dist/eause.dev.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "sass src/sass/main.scss:dist/eause.comp.css",
    "concat:css": "concat -o dist/eause.concat.css dist/eause.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' dist/eause.concat.css -o dist/eause.prefix.css",
    "expand:css": "sass dist/eause.prefix.css:dist/eause.css --style expanded",
    "compress:css": "sass dist/eause.css:dist/eause.min.css --style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css expand:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rckryd/eause.git"
  },
  "keywords": [
    "Eause",
    "CSS",
    "Framework"
  ],
  "author": "Ricky Riyadi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rckryd/eause/issues"
  },
  "homepage": "http://eause.rckryd.com",
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "concat": "^1.0.3",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3"
  }
}
